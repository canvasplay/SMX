{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_",
        "exclude": [
            "node_modules",
            "**/_*.*"
        ],
        "include":[
            "src",
            "README.md"
        ]
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "applicationName": "jsdocs-test"
    },
    "opts": {
        "destination": "./docs/",
        "sort": false,
        "template": "docs_template"
        //"template": "./node_modules/@pixi/jsdoc-template"
    }
}