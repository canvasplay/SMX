{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_",
        "exclude": [
            "node_modules",
            "**/_*.*"
        ],
        "include":[
            "src",
            "README.md"
        ]
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "applicationName": "smx"
    },
    "opts": {
        "destination": "./docs/",
        "sort": false,
        "mainpagetitle": "smx",
        "template": "./node_modules/smx-jsdoc-template"
    }
}